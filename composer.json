{
    "name": "yiisoft/mutex-db-redis",
    "type": "library",
    "description": "Yii Mutex Library - Redis DB Driver",
    "keywords": [
        "yii",
        "mutex"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sergey Makinen",
            "email": "sergey@makinen.ru"
        },
        {
            "name": "Alexander Zhuravlev",
            "email": "axelhex@gmail.com"
        }
    ],
    "support": {
        "source": "https://github.com/yiisoft/mutex-db-redis",
        "issues": "https://github.com/yiisoft/mutex-db-redis/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "require": {
        "yiisoft/mutex": "^3.0@dev",
        "yiisoft/yii-core": "^3.0@dev",
        "yiisoft/db-redis": "dev-master",
        "ext-json": "*"
    },
    "require-dev": {
        "yiisoft/log": "^3.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev",
        "phpunit/phpunit": "^7.3 || ^8.0",
        "squizlabs/php_codesniffer": "^3.2",
        "hiqdev/composer-config-plugin": "^1.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Mutex\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Mutex\\Tests\\": ["tests", "vendor/yiisoft/mutex/tests"]
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
